/* address.wxss */
.goodsSpec {
  height: 100rpx;
  width: 100%;
  padding: 0 24rpx;
  font-size: 26rpx;
  line-height: 100rpx;
  color: #333;
  border-bottom: 1rpx solid #eee;
  display: block;
}

icon{
  display: block;
  box-sizing: border-box;
  width: 45rpx;
  height: 45rpx;
}

.goodsSelect {
  height: 100%;
  padding: 29rpx 16rpx;
  box-sizing: border-box;
  width: 77rpx;
}

.notChoose{
  width: 45rpx;
  height: 45rpx;
  display: block;
  background-color: #eee;
  border-radius: 50%;
}